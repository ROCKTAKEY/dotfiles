(deftheme my-light
  "my light theme.")

(custom-theme-set-faces
 'my-light
 '(cursor ((t (:background "#555555"))))
 '(default ((t (:background "#fdf6e3" :foreground "#555555"))))
 '(region ((t (:background "LightGoldenrod"))))
 '(error ((t (:foreground "red" :bold t))))
 '(font-lock-builtin-face       ((t (:foreground "LightSteelBlue"))))
 '(font-lock-string-face ((t (:foreground "#be5565"))))
 '(font-lock-comment-face ((t (:foreground "#e55555"))))
 '(font-lock-keyword-face ((t (:foreground "#d343f3"))))
 '(font-lock-function-name-face ((t (:foreground "#4444ff"))))
 '(font-lock-constant-face ((t (:foreground "#22adad"))))
 '(font-lock-variable-name-face ((t (:foreground "sienna"))))
 '(font-lock-type-face ((t (:foreground "forest green"))))
 '(highlight-indent-guides-odd-face ((t (:background "misty rose"))))
 '(highlight-indent-guides-even-face ((t (:background "light cyan"))))
 '(highlight-indent-guides-character-face ((t (:foreground "dark gray"))))
 '(highlight-defined-function-name-face ((t (:foreground "#6a5acd"))))
 '(highlight-defined-variable-name-face ((t :foreground "sienna")))
 '(highlight-defined-face-name-face ((t (:foreground "#b8860b"))))
 )
